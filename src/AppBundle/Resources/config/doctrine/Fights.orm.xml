<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Fights" table="fights">
    <indexes>
      <index name="questId_idx" columns="quest_id"/>
      <index name="fightzone_id_idx" columns="fight_zone_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="quest" target-entity="Quests" fetch="LAZY">
      <join-columns>
        <join-column name="quest_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fightZone" target-entity="Map" fetch="LAZY">
      <join-columns>
        <join-column name="fight_zone_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
