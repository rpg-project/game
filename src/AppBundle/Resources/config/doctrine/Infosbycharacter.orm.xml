<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Infosbycharacter" table="infosByCharacter">
    <indexes>
      <index name="characId_idx" columns="characterId"/>
      <index name="infoId_idx" columns="infoId"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="infoid" target-entity="Infos" fetch="LAZY">
      <join-columns>
        <join-column name="infoId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="characterid" target-entity="Characters" fetch="LAZY">
      <join-columns>
        <join-column name="characterId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
