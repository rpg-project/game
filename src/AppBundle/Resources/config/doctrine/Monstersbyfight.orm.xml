<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Monstersbyfight" table="monstersByFight">
    <indexes>
      <index name="monsterId_idx" columns="monster_id"/>
      <index name="f_id_idx" columns="fight_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="number" type="integer" column="number" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="monster" target-entity="Monsters" fetch="LAZY">
      <join-columns>
        <join-column name="monster_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fight" target-entity="Fights" fetch="LAZY">
      <join-columns>
        <join-column name="fight_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
