<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Capacities" table="capacities">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="range" type="integer" column="range" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="attack" type="integer" column="attack" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="defense" type="integer" column="defense" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="move" type="integer" column="move" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quickness" type="integer" column="quickness" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="health" type="integer" column="health" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="energy" type="integer" column="energy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
