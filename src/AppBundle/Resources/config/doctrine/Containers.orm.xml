<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Containers" table="containers">
    <indexes>
      <index name="itemId_idx" columns="itemsId"/>
      <index name="containerId_idx" columns="containerId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="weigth" type="integer" column="weigth" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="itemsid" type="integer" column="itemsId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="containerid" type="integer" column="containerId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="characterdid" type="integer" column="characterdId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
