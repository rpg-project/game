<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Followersitems" table="FollowersItems">
    <indexes>
      <index name="followerId_idx" columns="FollowersId"/>
      <index name="itemId_idx" columns="ItemId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="equiped" type="integer" column="Equiped" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="itemid" target-entity="Items" fetch="LAZY">
      <join-columns>
        <join-column name="ItemId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="followersid" target-entity="Followers" fetch="LAZY">
      <join-columns>
        <join-column name="FollowersId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
