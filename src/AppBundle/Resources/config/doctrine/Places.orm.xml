<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Places" table="places">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="description" type="string" column="description" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="summon" type="integer" column="summon" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quest" type="integer" column="quest" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="training" type="integer" column="training" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="craft" type="integer" column="craft" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="merchant" type="integer" column="merchant" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="arena" type="integer" column="arena" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="healing" type="integer" column="healing" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="info" type="integer" column="info" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
