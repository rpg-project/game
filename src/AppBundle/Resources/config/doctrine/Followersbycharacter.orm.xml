<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Followersbycharacter" table="followersByCharacter">
    <indexes>
      <index name="characterId_idx" columns="characterId"/>
      <index name="followerId_idx" columns="followerId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="teamed" type="integer" column="teamed" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="followerid" target-entity="Followers" fetch="LAZY">
      <join-columns>
        <join-column name="followerId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="characterid" target-entity="Characters" fetch="LAZY">
      <join-columns>
        <join-column name="characterId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
