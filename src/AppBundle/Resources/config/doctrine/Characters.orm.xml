<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Characters" table="characters">
    <indexes>
      <index name="userId_idx" columns="userId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gold" type="integer" column="gold" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="health" type="integer" column="health" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxHealth" type="integer" column="max_health" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="stamina" type="integer" column="stamina" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxStamina" type="integer" column="max_stamina" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="energy" type="integer" column="energy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxEnergy" type="integer" column="max_energy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="move" type="integer" column="move" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quickness" type="integer" column="quickness" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="attack" type="integer" column="attack" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="defense" type="integer" column="defense" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="critical" type="integer" column="critical" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="level" type="integer" column="level" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="xp" type="integer" column="xp" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="location" type="integer" column="location" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="repopLocation" type="integer" column="repop_location" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="glory" type="integer" column="glory" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="faith" type="integer" column="faith" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="craftSkill" type="integer" column="craft_skill" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="law" type="integer" column="law" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="chaos" type="integer" column="chaos" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="good" type="integer" column="good" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="evil" type="integer" column="evil" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="warRank" type="integer" column="war_rank" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="arenaRank" type="integer" column="arena_rank" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="boxSize" type="integer" column="box_size" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxBoxSize" type="integer" column="max_box_size" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="lastLogin" type="datetime" column="last_login" nullable="true"/>
    <field name="title" type="integer" column="title" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxBagCapacity" type="integer" column="max_bag_capacity" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="userid" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="userId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
