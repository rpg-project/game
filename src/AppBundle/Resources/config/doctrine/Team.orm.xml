<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Team" table="team">
    <indexes>
      <index name="characterId_idx" columns="character_id"/>
      <index name="mate_idx" columns="team_mate_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="place" type="integer" column="place" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="avalaible" type="integer" column="avalaible" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="teamMate" target-entity="Followersbycharacter" fetch="LAZY">
      <join-columns>
        <join-column name="team_mate_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="character" target-entity="Characters" fetch="LAZY">
      <join-columns>
        <join-column name="character_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
