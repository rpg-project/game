<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Team" table="team">
    <indexes>
      <index name="characterId_idx" columns="character_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="place" type="integer" column="place" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="teamMateId" type="integer" column="team_mate_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="characterId" type="integer" column="character_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
