<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Itemsbyfollowers" table="itemsByFollowers">
    <indexes>
      <index name="followerId_idx" columns="followerId"/>
      <index name="itemId_idx" columns="itemId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="equiped" type="integer" column="equiped" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="contained" type="integer" column="contained" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="containerSpace" type="integer" column="container_space" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="containerid" type="integer" column="containerId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="levelMin" type="integer" column="level_min" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="level" type="integer" column="level" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quality" type="string" column="quality" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bonusMove" type="integer" column="bonus_move" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bonusQuickness" type="integer" column="bonus_quickness" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bonusAttack" type="integer" column="bonus_attack" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bonusDefense" type="integer" column="bonus_defense" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bonusCritical" type="integer" column="bonus_critical" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bonusHealth" type="integer" column="bonus_health" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="bonusEnergy" type="integer" column="bonus_energy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="capacity" type="integer" column="capacity" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="priceBuy" type="integer" column="price_buy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="priceSell" type="integer" column="price_sell" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="open" type="integer" column="open" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="weigth" type="integer" column="weigth" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="itemid" target-entity="Items" fetch="LAZY">
      <join-columns>
        <join-column name="itemId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="followerid" target-entity="Followersbycharacter" fetch="LAZY">
      <join-columns>
        <join-column name="followerId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
