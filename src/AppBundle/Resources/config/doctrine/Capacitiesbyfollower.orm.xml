<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Capacitiesbyfollower" table="capacitiesByFollower">
    <indexes>
      <index name="followerId_idx" columns="followerId"/>
      <index name="capacityId_idx" columns="capacityId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="followerid" target-entity="Followers" fetch="LAZY">
      <join-columns>
        <join-column name="followerId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="capacityid" target-entity="Capacities" fetch="LAZY">
      <join-columns>
        <join-column name="capacityId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
