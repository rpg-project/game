<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Monsters" table="monsters">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="health" type="integer" column="health" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="energy" type="integer" column="energy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="move" type="integer" column="move" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quickness" type="integer" column="quickness" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="attack" type="integer" column="attack" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="defense" type="integer" column="defense" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="critical" type="integer" column="critical" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="level" type="integer" column="level" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateInfo" type="datetime" column="date_info" nullable="true"/>
    <field name="kill" type="integer" column="kill" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="killTitle" type="integer" column="kill_title" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
