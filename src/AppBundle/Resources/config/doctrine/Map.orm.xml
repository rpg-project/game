<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Map" table="map">
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mapName" type="string" column="map_name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="width" type="string" column="width" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="height" type="string" column="height" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mapContent" type="text" column="map_content" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="keyId" type="integer" column="key_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateInfo" type="datetime" column="date_info" nullable="true"/>
  </entity>
</doctrine-mapping>
