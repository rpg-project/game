<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Followersbycharacter" table="followersByCharacter">
    <indexes>
      <index name="characterId_idx" columns="characterId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="teamed" type="integer" column="teamed" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="health" type="integer" column="health" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxHealth" type="integer" column="max_health" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="energy" type="integer" column="energy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxEnergy" type="integer" column="max_energy" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="move" type="integer" column="move" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quickness" type="integer" column="quickness" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="attack" type="integer" column="attack" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="defense" type="integer" column="defense" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="critical" type="integer" column="critical" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="level" type="integer" column="level" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="xp" type="integer" column="xp" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="image" type="string" column="image" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="goal" type="integer" column="goal" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="rateLabel" type="string" column="rate_label" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="uniqueRate" type="integer" column="unique_rate" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="priceBack" type="integer" column="price_back" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="followerid" type="integer" column="followerId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="law" type="integer" column="law" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="chaos" type="integer" column="chaos" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="good" type="integer" column="good" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="evil" type="integer" column="evil" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="maxCapacityBag" type="integer" column="max_capacity_bag" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="characterid" target-entity="Characters" fetch="LAZY">
      <join-columns>
        <join-column name="characterId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
