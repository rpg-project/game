<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Itemsbycharacter" table="itemsByCharacter">
    <indexes>
      <index name="characterId_idx" columns="characterId"/>
      <index name="itemId_idx" columns="itemId"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="equiped" type="integer" column="equiped" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="contained" type="integer" column="contained" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="containerSpace" type="integer" column="container_space" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="characterid" type="integer" column="characterId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="itemid" target-entity="Items" fetch="LAZY">
      <join-columns>
        <join-column name="itemId" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
